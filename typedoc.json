{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/"],
  "entryPointStrategy": "resolve",
  "plugin": ["typedoc-plugin-missing-exports"],
  "gitRevision": "main",
  "gitRemote": "origin",
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeReferences": true,
  "categoryOrder": ["Getting Started", "Authentication", "Videos", "Playlists", "Webcasts", "Users & Groups", "Channels", "Administration", "Devices", "Audit"],
  "categorizeByGroup": false,
  "outputs": [
    {
      "name": "html",
      "path": "./docs",
      "options": {
          "navigation": {
              "includeCategories": true,
              "includeGroups": true,
              "excludeReferences": true,
              "includeFolders": true
          }
      }
    }
  ],
  // typedoc.json
  "kindSortOrder": [
      "Reference",
      "Project",
      "Module",
      "Variable",
      "Interface",
      "Class",
      "Namespace",
      "Enum",
      "EnumMember",
      "TypeAlias",
      "Constructor",
      "Property",
      "Function",
      "Accessor",
      "Method",
      "Parameter",
      "TypeParameter",
      "TypeLiteral",
      "CallSignature",
      "ConstructorSignature",
      "IndexSignature",
      "GetSignature",
      "SetSignature"
  ],
  "placeInternalsInOwningModule": true
}
